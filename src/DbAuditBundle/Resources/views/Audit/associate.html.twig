<tr>
  <td class="col-min">
    <span class="label label-success"><i class="fa fa-random"></i></span>
  </td>
  <td>
    <strong>{{ log.source.typLabel }}</strong> {{ db_audit_assoc(log.source) }} was associated with

    <strong>{{ log.target.typLabel }}</strong> {{ db_audit_assoc(log.target) }},
    {{ db_audit_blame(log.blame) }}
  </td>
  <td class="col-min">{#{{ time_diff(log.loggedAt) }}#} {{ log.loggedAt | time_diff_personal }}</td>
  <td class="col-min"></td>
</tr>
