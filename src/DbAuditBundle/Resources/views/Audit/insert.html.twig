<tr>
  <td class="col-min">
    <span class="label label-success"><i class="fa fa-plus"></i></span>
  </td>
  <td>
    New <strong>{{ log.source.typLabel }}</strong> {{ db_audit_assoc(log.source) }} was inserted,
    {{ db_audit_blame(log.blame) }}
  </td>
  <td class="col-min">{{ log.loggedAt | time_diff_personal  }}</td>
  <td class="col-min">
    <a class="btn btn-xs btn-link" href="{{ path('databaseaudit_diff', {id: log.id}) }}">
      <i class="fa fa-search" title="Show diff"></i>
    </a>
  </td>
</tr>
