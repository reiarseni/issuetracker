{
    "name": "unknown/issuetracker",
    "license": "MIT",
    "type": "project",
    "description": "System",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.4.*",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^3.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/assetic-bundle": "^2.8",
        "symfony/finder": "~2.1",
        "twig/twig": "^1.0||^2.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "stof/doctrine-extensions-bundle": "~1.2",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "friendsofsymfony/jsrouting-bundle": "~1.6",
        "friendsofsymfony/rest-bundle": "~1.1",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "knplabs/knp-paginator-bundle": "~2.5",
        "knplabs/knp-menu-bundle": "~2.1.3",
        "jms/translation-bundle": "^1.3",
        "tecnickcom/tcpdf": "~6.0",
        "phpoffice/phpexcel": "~1.8",
        "incenteev/composer-parameter-handler": "^2.0",
        "liip/imagine-bundle": "~1.6",
        "vich/uploader-bundle": "~1.4",
        "raulfraile/ladybug-bundle": "~1.0",
        "javiereguiluz/easyadmin-bundle": "~1.16",
        "egeloen/ckeditor-bundle" : "^4.0",
        "jbouzekri/file-uploader-bundle": "~4.0",
        "anacona16/imagecrop-bundle": "^2.4",
        "jms/serializer-bundle": "^2.2",
        "nelmio/cors-bundle": "^1.5",
        "nelmio/api-doc-bundle": "^2.13",
        "apy/datagrid-bundle": "^3.1",
        "easycorp/easyadmin-bundle": "^1.17",
        "apy/breadcrumbtrail-bundle": "^1.5",
        "ninsuo/symfony-collection": "^2.1",
        "kijho/kijho-chat": "dev-master",
        "hhxsv5/php-sse": "~1.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "~2.2@dev",
        "symfony/phpunit-bridge": "^3.0",
        "phpunit/phpunit": "~5.7",
        "elao/web-profiler-extra-bundle": "~2.3",
        "fzaninotto/faker": "^1.6",
        "dama/doctrine-test-bundle": "^4.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
