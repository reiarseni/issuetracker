
{{ form_start(form) }}

<div class="row">
    <div class="col-lg-3">{{ form_row(form.username) }}</div>
    <div class="col-lg-3">{{ form_row(form.email) }}</div>
    <div class="col-lg-3">{{ form_row(form.plainPassword) }} </div>
    <div class="col-lg-3">{{ form_row(form.roles) }}</div>
